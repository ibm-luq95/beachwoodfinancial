  "version": "0.2.0",
  "configurations": [
    {
      "name": "BeachWoodFinancial Python: Django",
      "type": "python",
      "stopOnEntry": true,
      "request": "launch",
      "program": "${workspaceFolder}/src/manage.py",
      "console": "integratedTerminal",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true,
      "redirectOutput": true,
      "showReturnValue": true,
      "cwd": "${workspaceFolder}/src",
      "autoReload": {"enable": true}
    //   "debugOptions": [
    //     "WaitOnAbnormalExit",
    //     "WaitOnNormalExit",
    //     "RedirectOutput",
    //     "DjangoDebugging"
    //   ]
    }
  ]
}

    "@preline/accordion": "^1.3.0",
    "@preline/collapse": "^1.3.0",
    "@preline/dropdown": "^1.3.0",
    "@preline/overlay": "^1.4.0",
    "@preline/remove-element": "^1.3.0",
    "@preline/scrollspy": "^1.3.0",
    "@preline/tabs": "^1.3.1",
    "@preline/tooltip": "^1.4.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.1.3",
    "chart.js": "^4.3.0",
    "core-js": "^3.20.3",
    "css.gg": "^2.0.0",
    "postcss": "^8.4.23",
    "preline": "^1.8.0",
    "tailwindcss": "^3.3.2"
  }
}
